@model Message

<header>
  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
      <!-- Slide One - Set the background image for this slide in the line below -->
      <div class="carousel-item active" style="background-image: url('../images/Mrs Agile 2nd layout.jpg')">
        <div class="carousel-caption">
          <h2 class="display-4 captionMain1">Create <span class="captionPart">It</span></h2>
          <p class="lead">.</p>
        </div>
      </div>
      <!-- Slide Two - Set the background image for this slide in the line below -->
      <div class="carousel-item" style="background-image: url('../images/Mrs Agile 3rd trail.jpg')">
        <div class="carousel-caption">
          <h2 class="display-4 captionMain2">Design <span class="captionPart">It</span></h2>
          <p class="lead">.</p>
        </div>
      </div>
      <!-- Slide Three - Set the background image for this slide in the line below -->
      <div class="carousel-item" style="background-image: url('../images/MrsAgile4thTrail.jpg')">
        <div class="carousel-caption">
          <h2 class="display-4 captionMain3">Build <span class="captionPart">It</span></h2>
          <p class="lead">.</p>
        </div>
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
  </div>
</header>

<!-- Page Content -->
<section class="py-5">
  <div class="container">
    <p class="display-4">@Model.MessageSummary</p>
  </div>
</section>